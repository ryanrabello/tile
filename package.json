{
  "name": "tile",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "deploy": "yarn build --base=/tile/ && touch dist/.nojekyll && git add dist/ && git commit -m \"Deploy gh-pages\" && git subtree push --prefix dist origin gh-pages"
  },
  "devDependencies": {
    "prettier": "^2.7.1",
    "typescript": "^4.6.4",
    "vite": "^3.2.3"
  },
  "dependencies": {
    "@types/d3": "^7.4.0",
    "d3": "^7.6.1",
    "pixi.js": "^7.0.4",
    "simplex-noise": "^4.0.1"
  }
}
